BEGIN
{
    printf("Tracing block I/O errors... Hit Ctrl-C to end.\n");
}

t:block:block_rq_complete
/args->error != 0/
{
    time("%H:%M:%S");
    printf("device: %d, %d, sector: %d, bytes: %d, flags: %s, error: %d\n", args->dev >> 20, ((args->dev) & (1 << 20) - 1), args->sector, args->nr_sector * 512, args->rwbs, args->error);
}