BEGIN
{
    printf("Tracing block I/o with init stacks. Hit Ctrl-C to end.\n");
}

//k:blk_account_io_start
k:blk_execute_rq_nowait
{
    @reqstack[arg0] = kstack;
    @reqts[arg0] = nsecs;
}

//k:blk_start_request,
k:blk_mq_start_request
/@reqts[arg0]/
{
    @usecs[@reqstack[arg0]] = hist(nsecs - @reqts[arg0]);
    delete(@reqstack[arg0]);
    delete(@reqts[arg0]);
}

END
{
    clear(@reqstack);
    clear(@reqts);
}

